{
    "name": "discord-nest",
    "version": "1.0.0",
    "description": "Discord backend clone in NestJS",
    "author": "Semih Berkay Ozturk",
    "private": true,
    "license": "MIT",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "cross-env NODE_ENV=development nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "cross-env NODE_ENV=test jest --watch --maxWorkers=1",
        "test:cov": "jest --coverage",
        "pre-commit": "lint-staged",
        "prepare": "node_modules/.bin/husky-run install",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.ts": [
            "npm run format",
            "eslint --ext .ts \"{src,apps,libs,test}/**/*.ts\" --fix"
        ]
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.391.0",
        "@nestjs/bull": "^10.0.1",
        "@nestjs/cache-manager": "^2.1.0",
        "@nestjs/common": "^10.1.3",
        "@nestjs/config": "^3.0.0",
        "@nestjs/core": "^10.1.3",
        "@nestjs/jwt": "^10.1.0",
        "@nestjs/passport": "^10.0.0",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/schedule": "^3.0.3",
        "@nestjs/sequelize": "^10.0.0",
        "@nestjs/swagger": "^7.1.7",
        "@nestjs/throttler": "^4.2.1",
        "@opentelemetry/auto-instrumentations-node": "^0.39.1",
        "@opentelemetry/exporter-jaeger": "^1.15.2",
        "@opentelemetry/exporter-trace-otlp-http": "^0.41.2",
        "@opentelemetry/instrumentation-express": "^0.33.1",
        "@opentelemetry/instrumentation-http": "^0.41.2",
        "@opentelemetry/instrumentation-nestjs-core": "^0.33.1",
        "@opentelemetry/instrumentation-pg": "^0.36.1",
        "@opentelemetry/instrumentation-winston": "^0.32.1",
        "@opentelemetry/propagator-b3": "^1.15.2",
        "@opentelemetry/resources": "^1.15.2",
        "@opentelemetry/sdk-node": "^0.41.2",
        "@opentelemetry/sdk-trace-base": "^1.15.2",
        "@opentelemetry/semantic-conventions": "^1.15.2",
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-session": "^2.0.44",
        "@types/multer": "^1.4.7",
        "@willsoto/nestjs-prometheus": "^5.3.0",
        "aws-sdk": "^2.1437.0",
        "bcrypt": "^5.1.1",
        "bull": "^4.11.3",
        "cache-manager": "^5.2.3",
        "cache-manager-redis-store": "^3.0.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cookie-session": "^2.0.0",
        "dotenv": "^16.3.1",
        "express-status-monitor": "^1.3.4",
        "heapdump": "^0.3.15",
        "helmet": "^7.0.0",
        "ioredis": "^5.3.2",
        "multer": "^1.4.5-lts.1",
        "nest-winston": "^1.9.3",
        "opentelemetry-instrumentation-sequelize": "^0.39.1",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.11.2",
        "pg-hstore": "^2.3.4",
        "prom-client": "^14.2.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "sequelize": "^6.32.1",
        "sequelize-typescript": "^2.1.5",
        "winston": "^3.10.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/cache-manager": "^4.0.2",
        "@types/cache-manager-redis-store": "^2.0.1",
        "@types/cron": "^2.4.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.1",
        "@types/passport-jwt": "^3.0.9",
        "@types/passport-local": "^1.0.35",
        "@types/sequelize": "^4.28.15",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.59.11",
        "@typescript-eslint/parser": "^5.59.11",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^4.3.8",
        "jest": "^29.5.0",
        "lint-staged": "^13.2.3",
        "prettier": "^2.8.8",
        "sequelize-cli": "^6.6.1",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-mockito": "^2.6.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}